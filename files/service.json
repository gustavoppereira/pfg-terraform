  [{
    "name": "web",
    "image": "gustavoppereira/pfg-cloud:latest",
    "cpu": 1000,
    "memory": 512,
    "portMappings": [{
      "containerPort": 5000,
      "hostPort": 5000
    }],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/service",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "app"
      }
    },
    "environment": [{
      "name": "DB_HOST",
      "value": "${DB_HOST}"
    },
    {
      "name": "PROVIDER",
      "value": "aws"
    },
    {
      "name": "TEST_GROUP_ID",
      "value": "${TEST_GROUP_ID}"
    }]
  }]
