  [{
    "name": "prometheus",
    "image": "prom/prometheus:latest",

    "memory": 512,
    "portMappings": [{
      "containerPort": 9090,
      "hostPort": 9090
    }],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/monitor",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "prometheus"
      }
    }
  }]
